{
    "name": "tjmugova/laravel-dpo",
    "description": "Provides DPO Payment integration for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Tererai Mugova",
            "email": "tjmugova@webstudio.co.zw"
        }
    ],
    "keywords": [
        "laravel-dpo",
        "dpo"
    ],
    "require": {
        "php": ">=7.4|^8.0",
        "illuminate/notifications": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/events": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/queue": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tjmugova\\Dpo\\": "src",
            "Tjmugova\\Dpo\\Tests\\": "tests"
        },
        "files": []
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tjmugova\\Dpo\\DpoServiceProvider"
            ],
            "aliases": {
                "Dpo": "Tjmugova\\Dpo\\Facades\\Dpo"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    }
}
